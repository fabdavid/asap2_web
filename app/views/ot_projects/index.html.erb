<p id="notice"><%= notice %></p>

<h1>Ot Projects</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ot_projects.each do |ot_project| %>
      <tr>
        <td><%= link_to 'Show', ot_project %></td>
        <td><%= link_to 'Edit', edit_ot_project_path(ot_project) %></td>
        <td><%= link_to 'Destroy', ot_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ot Project', new_ot_project_path %>
