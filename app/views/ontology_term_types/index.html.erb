<p id="notice"><%= notice %></p>

<h1>Ontology Term Types</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ontology_term_types.each do |ontology_term_type| %>
      <tr>
        <td><%= link_to 'Show', ontology_term_type %></td>
        <td><%= link_to 'Edit', edit_ontology_term_type_path(ontology_term_type) %></td>
        <td><%= link_to 'Destroy', ontology_term_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ontology Term Type', new_ontology_term_type_path %>
